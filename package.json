{
  "name": "webpage",
  "version": "1.0.0",
  "description": "homepage",
  "private": true,
  "scripts": {
    "serve-functions": "netlify-lambda serve --port 8082 functions-src",
    "build-functions": "netlify-lambda build functions-src",
    "serve": "npm run serve-functions | parcel index.html --port $PORT --log-level 3 --hmr-port 8081",
    "prebuild": "rm -rf dist",
    "prebuild-keep": "rm -rf dist && cp -R ./assets ./dist",
    "build": "npm run build-functions | parcel build --no-content-hash index.html",
    "bundle-experiment": "node bundler.js"
  },
  "devDependencies": {
    "cssnano": "^4.1.7",
    "netlify-lambda": "^1.0.2",
    "parcel-bundler": "^1.10.3",
    "posthtml-expressions": "^1.1.0",
    "sass": "^1.14.3"
  }
}
